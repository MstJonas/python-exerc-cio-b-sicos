cont = 0
conta = 0
c = 0
soma = 0
for k in range(1,101):
    if k % 3 == 0:
        cont +=1
    if k % 5 == 0:
        conta +=1
    if k % 3 == 0 and k % 5 == 0:
        c +=1
    if k % 3 != 0 and k > 50:
        soma += k
print(f'Quantidade de multiplo de 3{cont} \nQuantidade de multiplo de 5{conta}\nMultiplos de 3 e 5 {c}\nSoma final {soma}')
